<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".act_buy_manage">

    <EditText
        android:id="@+id/edtitemcode"
        android:layout_width="248dp"
        android:layout_height="45dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="120dp"
        android:ems="18"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_Cfname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="196dp"

        android:layout_marginTop="672dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_ID"
        android:layout_width="55dp"
        android:layout_height="16dp"
        android:layout_marginStart="252dp"
        android:layout_marginTop="700dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_Ccontact"
        android:layout_width="55dp"
        android:layout_height="16dp"
        android:layout_marginStart="320dp"
        android:layout_marginTop="700dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_buyername"
        android:layout_width="55dp"
        android:layout_height="16dp"
        android:layout_marginStart="320dp"
        android:layout_marginTop="700dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_buyerqty"
        android:layout_width="55dp"
        android:layout_height="16dp"
        android:layout_marginStart="320dp"
        android:layout_marginTop="700dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_buyercontact"
        android:layout_width="55dp"
        android:layout_height="16dp"
        android:layout_marginStart="320dp"
        android:layout_marginTop="700dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_Pqty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="672dp"
        android:layout_marginEnd="232dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_Pname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="696dp"
        android:layout_marginEnd="32dp"

        android:text="TextView"
        app:layout_constraintEnd_toStartOf="@+id/txt_Pqty"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_Clname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="672dp"
        android:layout_marginEnd="32dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_Pprice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="672dp"
        android:layout_marginEnd="104dp"
        android:text="TextView"

        app:layout_constraintEnd_toStartOf="@+id/txt_Cfname"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnQuery"
        android:layout_width="98dp"
        android:layout_height="55dp"
        android:layout_marginStart="152dp"
        android:layout_marginBottom="500dp"
        android:text="search"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ListView
        android:id="@+id/listview"
        android:layout_width="342dp"
        android:layout_height="408dp"
        android:layout_marginBottom="76dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintHorizontal_bias="0.873"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtitemcode" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="428dp"
        android:layout_height="95dp"
        android:layout_marginTop="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

</androidx.constraintlayout.widget.ConstraintLayout>